{
	"name": "diploma-monorepo",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"build:backend": "pnpm --filter backend run build",
		"build:frontend": "pnpm --filter frontend run build",
		"build": "pnpm run build:shared && pnpm run build:backend && pnpm run build:frontend",
		"backend": "pnpm --filter backend run start:dev",
		"frontend": "pnpm --filter frontend run dev",
		"dev": "concurrently \"pnpm run backend\" \"pnpm run frontend\""
	},
	"devDependencies": {
		"concurrently": "^9.1.0",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@prisma/client": "7.1.0",
		"prisma": "7.1.0"
	}
}
